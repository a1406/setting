    (load "~/.emacs.conf/other.lisp")
  )
(load "~/.emacs.conf/spacemacs-func.lisp")
(load "~/.emacs.conf/emacs_key.conf")

(setq custom-file "~/.custom-file")
(if (file-exists-p custom-file)
    (load custom-file)
  )

(if (eq custom-enabled-themes nil)
